local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("poster sus")

local serv = win:Server("AUT", "")

local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
function CLICK()
    local args = {
        [1] = "KEY",
        [2] = "LMB"
    }
    
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(unpack(args))
    
    
    local args = {
        [1] = "LMB"
    }
    
    game:GetService("ReplicatedStorage").Remotes.InputFunc:InvokeServer(unpack(args))
    
    
    local args = {
        [1] = "KEY",
        [2] = "END-LMB"
    }
    
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(unpack(args))
end
local mob = serv:Channel("Farm Mob")

mob:Dropdown("SELECT MOB", {"Akira_DEV Dummy", "Thug" , "KillableDummy" , "SansDummy", "ReflectingDummy" , "BlockingDummy" , "Scorpion", "Umbra"}, function(concac)
    getgenv().mobb = concac
end)

des = 10

mob:Toggle("Farm Mob", false, function(g)
    getgenv().start = g
    function gg()
        pcall(function()
            for i,v in pairs(game:GetService("Workspace").Living:GetChildren()) do
                if v.Name == getgenv().mobb then
                    repeat
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,6)
                        CLICK()
                    until v.Humanoid.Health == 0 or getgenv().start == false
                end
            end
        end)
    end

    while getgenv().start do wait(0.1)
        gg()
    end


end)
mob:Toggle("God mode",false, function(god)
    getgenv().god = god
    while getgenv().god do wait()
        pcall(function()
            for i, v in pairs(game.Players.LocalPlayer.Character.Values:GetChildren()) do
                if v.Name == "Block" then
                v:Destroy()
                wait(4)
                game:GetService("VirtualInputManager"):SendKeyEvent(true, "X", false, game)
                end
            end
        end)
    end
end)
mob:Label("only turn godmode for mob farm cuz its suck")
local tgls = serv:Channel("Farm Item")

local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)

tgls:Button("TP To Safe Zone",function()
    Instance.new("Part",game:GetService("Workspace").ServerInformation)
    for i,v in pairs(game:GetService("Workspace").ServerInformation:GetChildren()) do
        if v.Name == "Part" then
            v.CFrame = CFrame.new(294.95635986328, 2569.0732421875, -0.31307902932167)
            v.Size = Vector3.new(60, 3, 65)
            v.Position = Vector3.new(294.956, 2669.07, -1.31308)
            v.Anchored = true
        end
    end
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(294.955994, 2673.07007, -1.31307995, 1, 0, 0, 0, 1, 0, 0, 0, 1)
end)


tgls:Toggle("Just Turn This On When Use ItemFarm",false, function(b)
    getgenv().noclip = b
    game:GetService('RunService').Stepped:connect(function()
        if getgenv().noclip then
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
        end
    end)
end)

function PRESS()
    game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game)
end

function CLICK()
    local args = {
        [1] = "KEY",
        [2] = "LMB"
    }
    
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(unpack(args))
    
    
    local args = {
        [1] = "LMB"
    }
    
    game:GetService("ReplicatedStorage").Remotes.InputFunc:InvokeServer(unpack(args))
    
    
    local args = {
        [1] = "KEY",
        [2] = "END-LMB"
    }
    
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(unpack(args))
end
  
local plr = game.Players.LocalPlayer

tgls:Toggle("Meteor Farm",false, function(bool)
    getgenv().FARM = bool
function concac()
    for _,x in pairs(game:GetService("Workspace").ItemSpawns.Chests:GetChildren()) do
        for i,v in pairs(game:GetService("Workspace").ItemSpawns.Meteors:GetChildren()) do
            if v:FindFirstChild("Meteor") and not x:FindFirstChild("Chest") then
                repeat
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,7,0)
                    PRESS()
                until not v:FindFirstChild("Meteor") or getgenv().FARM
            end
        end
    end
end

while getgenv().FARM do wait(0.1)
    concac()
end
end)

tgls:Toggle("Chest Farm",false, function(bool)
    getgenv().FARM1 = bool
function cc()
    for i,v in pairs(game:GetService("Workspace").ItemSpawns.Chests:GetChildren()) do
        if v:FindFirstChild("Chest") then
            repeat
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,7,0)
                PRESS()
            until not v:FindFirstChild("Chest") or getgenv().FARM1
        end
    end
end

while getgenv().FARM1 do wait(0.1)
    cc()
end
end)

tgls:Toggle("Item Farm",false, function(bool)
    getgenv().FARM2 = bool
        function c1c()
            for i,v in pairs(game:GetService("Workspace").ItemSpawns.StandardItems.SpawnLocation:GetChildren()) do
                if v:FindFirstChild("Dragon ball") or v:FindFirstChild("Ben 10") then
                    repeat
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,7,0)
                        PRESS()
                    until not v:FindFirstChild("Handle") or not getgenv().FARM2
                end
            end 
        end
    
    while getgenv().FARM2 do wait(0.1) 
        c1c()
    end
    end)

tgls:Toggle("SandDebris Farm",false, function(bool)
    getgenv().Hard = bool
function hard()
    for i,v in pairs(game:GetService("Workspace").ItemSpawns["Sand Debris"]:GetChildren()) do
        if v:FindFirstChild("SandDebris") then
            repeat
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame * CFrame.Angles(math.rad(90), 0, 0) - Vector3.new(0,7,0)
                PRESS()
            until not v:FindFirstChild("SandDebris") or not getgenv().Hard
        end
    end
end

while getgenv().Hard do wait(0.1)
    hard()
end
end)

tgls:Textbox("Webhook", "Type here!", true, function(t)
    getgenv().Webhook = t
end)

tgls:Label("Paste webhook and press Enter")

tgls:Toggle("Noti Item", false, function(t)
    getgenv().noti = t
    game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(v)
        if getgenv().noti then
            spawn(function()
                pcall(function()
                    local url = getgenv().Webhook
                    local data = {
                    ["content"] = "@everyone",
                    ["embeds"] = {
                        {
                            ["title"] = "ITEM U GET",
                            ["description"] = "Username: " .. game.Players.LocalPlayer.Name.. "\n\nItem: " ..  v.Name,
                            ["type"] = "rich",
                            ["color"] = tonumber(0x7269da),
                        }
                    }
                    }
                    local newdata = game:GetService("HttpService"):JSONEncode(data)
                
                    local headers = {
                    ["content-type"] = "application/json"
                    }
                    request = http_request or request or HttpPost or syn.request
                    local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
                    request(abcdef)
                end)
            end)
        end
    end)
end)

tgls:Button("Test Webhook", function()
                local url = getgenv().Webhook
                local data = {
                ["content"] = "@everyone",
                ["embeds"] = {
                    {
                        ["title"] = "Sussy Among us cock",
                        ["description"] = "Username: " .. game.Players.LocalPlayer.Name.. "\n\nTEST",
                        ["type"] = "rich",
                        ["color"] = tonumber(0x7269da),
                    }
                }
                }
                local newdata = game:GetService("HttpService"):JSONEncode(data)
            
                local headers = {
                ["content-type"] = "application/json"
                }
                request = http_request or request or HttpPost or syn.request
                local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
                request(abcdef)
end)
local skill = serv:Channel("Auto Skill")

