local tp = serv:Channel("Teleport")

getgenv().tpmob = {}
for i,v in pairs(game:GetService("Workspace").NPCS:GetChildren()) do
    table.insert(getgenv().tpmob ,v.Name)
end

tp:Dropdown("Select NPC", getgenv().tpmob, function(a)
    tpmob1 = a
end)

tpmob1 = ""


tp:Button("Tp NPC" , function()
    plr3 = game:GetService("Workspace").NPCS:FindFirstChild(tpmob1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plr3.HumanoidRootPart.CFrame
end)
